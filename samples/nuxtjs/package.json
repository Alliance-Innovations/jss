{
  "name": "nuxt-app",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start:production": "npm-run-all --serial bootstrap build start",
    "start:connected": "npm-run-all --serial bootstrap dev start:watch-components",
    "bootstrap": "ts-node --project tsconfig.scripts.json ./scripts/bootstrap.ts",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch"
  },
  "config": {
    "appName": "nuxt-app",
    "rootPlaceholders": [
      "nuxt-jss-main"
    ],
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "language": "en"
  },
  "dependencies": {
    "@nuxtjs/apollo": "^5.0.0-alpha",
    "@sitecore-jss/sitecore-jss": "^21.7.0-canary.49",
    "@sitecore-jss/sitecore-jss-vue": "^21.7.0-canary.49",
    "@types/debug": "^4.1.5",
    "body-parser": "^1.20.1",
    "bootstrap": "^5.1.3",
    "debug": "^4.3.1",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-request": "^6.0.0",
    "http-proxy-middleware": "^2.0.1",
    "memory-cache": "^0.2.0",
    "sync-disk-cache": "^2.1.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/register": "7.18.9",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@nuxtjs/i18n": "^8.0.0",
    "@sitecore-jss/sitecore-jss-cli": "^21.7.0-canary.49",
    "@sitecore-jss/sitecore-jss-dev-tools": "^21.7.0-canary.49",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/memory-cache": "^0.2.1",
    "constant-case": "^3.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.32.0",
    "graphql-tag": "^2.12.6",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.9.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite-plugin-graphql-loader": "^3.0.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5"
  }
}
