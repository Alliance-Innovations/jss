{
  "private": true,
  "config": {
    "appName": "nuxt-app",
    "rootPlaceholders": [
      "jss-main"
    ],
    "sitecoreDistPath": "/dist/nuxt-app",
    "sitecoreConfigPath": "/App_Config/Include/zzz",
    "graphQLEndpointPath": "/sitecore/api/graph/edge",
    "buildArtifactsPath": "./dist",
    "language": "en"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start:production": "node .output/server/index.mjs",
    "start": "cross-env-shell JSS_MODE=disconnected \"npm-run-all --serial bootstrap --parallel start:dev start:proxy start:watch-components\"",
    "start:dev": "nuxi dev",
    "bootstrap": "ts-node --project tsconfig.scripts.json ./scripts/bootstrap.ts",
    "start:proxy": "ts-node --project tsconfig.scripts.json scripts/disconnected-mode-proxy.ts",
    "start:watch-components": "ts-node --project tsconfig.scripts.json scripts/generate-component-factory.ts --watch"
  },
  "dependencies": {
    "@sitecore-jss/sitecore-jss-vue": "^21.7.0-canary.49",
    "body-parser": "^1.19.1",
    "bootstrap": "^5.1.3",
    "express": "^4.17.2",
    "http-proxy-middleware": "^2.0.1",
    "vue-i18n": "^9.1.9"
  },
  "devDependencies": {
    "@intlify/nuxt3": "^0.1.9",
    "@sitecore-jss/sitecore-jss-cli": "^21.7.0-canary.49",
    "@sitecore-jss/sitecore-jss-dev-tools": "^21.7.0-canary.49",
    "@types/http-proxy-middleware": "^1.0.0",
    "cross-env": "^7.0.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.9.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
